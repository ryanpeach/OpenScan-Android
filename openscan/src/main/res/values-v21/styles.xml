<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="Preview">
        <item name="android:elevation">@dimen/default_elev</item>
        <item name="android:layout_margin">@dimen/margin_size</item>
    </style>
    <style name="Settings">
        <item name="android:elevation">@dimen/default_elev</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:layout_margin">@dimen/margin_size</item>
        <item name="android:padding">@dimen/margin_size</item>
    </style>
    <style name="FAB" parent="Widget.AppCompat.ActionButton">
        <item name="android:layout_margin">@dimen/margin_size</item>
        <item name="android:elevation">@dimen/default_elev</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SeekBar" parent="Widget.AppCompat.SeekBar"></style>
    <style name="Spinner" parent="Widget.AppCompat.Spinner.DropDown"></style>
    <style name="Text" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">20sp</item>
    </style>
</resources>